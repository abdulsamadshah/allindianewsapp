<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/black"
    tools:context=".Tabnavigation.aajTakNewsFragment">


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/aajtakrecyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/uploadnews"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#557481"
        android:clickable="true"
        app:srcCompat="@drawable/uploadss"
       app:layout_constraintBottom_toBottomOf="@+id/aajtakrecyclerview"
        app:layout_constraintEnd_toEndOf="@+id/aajtakrecyclerview"
        app:layout_constraintHorizontal_bias="0.898"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.873" />

</androidx.constraintlayout.widget.ConstraintLayout>